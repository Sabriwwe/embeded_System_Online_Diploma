// code begining
#include <stdio.h>
#include <string.h>


// function body 
void draw (){
  int i =0 , j=0 ;
  // itrations
  for (i; i<10; i++){
    printf("\n ");
    for (j=i; j<10; j++){
      printf("%d ",j);
      }
    }
  }

int main(void){ 
// function calling
 draw() ;
  return 0 ;
}
