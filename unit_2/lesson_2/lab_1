#include <stdio.h>

int main()
{
    int x [10];
    for (int i =0 ; i <10 ; i++)
    {
        printf("Enter the student %d degree : ",i);
        scanf("%d",&x[i]);
    }
    
    for (int i =0 ; i <10 ; i++)
    {
        printf("\nStudent %d degree is : %d ", i , x[i]);
        
    }
    
    
    return 0;
}
